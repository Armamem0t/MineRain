      "type": "image",
      "size": [
         "100%",
         "80px"
      ],
      "texture": "textures/MineRain/black",
      "alpha": 0.3,
      "controls": [
         {
            "horizontal_stack_panel@common.horizontal_stack_panel": {
               "size": [
                  "100%-2px",
                  "100%-2px"
               ],
               "controls": [
                  {
                     "icon_panel@pack_info.icon_image": {
                        "size": [
                           "100%c+2px",
                           "100%"
                        ],
                        "type": "panel",
                        "controls": [
                           {
                              "icon": {
                                 "type": "image",
                                 "size": [
                                    "100%y",
                                    "100%-2px"
                                 ],
                                 "layer": 6,
                                 "texture": "textures/MineRain/pack_icon"
                              }
                           }
                        ]
                     }
                  },
                  {
                     "piddling_fill_icon": {
                        "type": "panel",
                        "size": [
                           3,
                           3
                        ]
                     }
                  },
                  {
                     "right_area_label_panel@common.vertical_stack_panel": {
                        "size": [
                           "fill",
                           "100%"
                        ],
                        "controls": [
                           {
                              "label_title@pack_info.common_label": {
                                 "$text_label": "($clientName+'ยง6UI')",
                                 "font_scale_factor": 2.8
                              }
                           },
                           {
                              "Version@pack_info.common_label": {
                                 "$text_label": "('Version '+$Vacation_Number)",
                                 "font_scale_factor": 1.3
                              }
                           },
                           {
                              "piddling_fill_copyright": {
                                 "type": "panel",
                                 "size": [
                                    3,
                                    5
                                 ]
                              }
                           },
                           {
                              "Release_date@pack_info.common_label": {
                                 "$text_label": "('Release-date '+$Release_date)",
                                 "font_scale_factor": 1.3
                              }
                           },
                           {
                              "piddling_fill_copyright": {
                                 "type": "panel",
                                 "size": [
                                    3,
                                    "fill"
                                 ]
                              }
                           },
                           {
                              "Copyright@pack_info.common_label": {
                                 "$text_label": "$Copyright",
                                 "font_scale_factor": 1.3
                              }
                           }
                        ]
                     }
                  }
               ]
            }
         }
