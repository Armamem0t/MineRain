      "layer": 5,
      "texture": "textures/MineRain/black",
      "controls": [
         {
            "common_label@pack_info.common_label": {}
         }
      ]
   },
   "common_label": {
      "type": "label",
      "max_size": [
         "100%",
         "100%c"
      ],
      "layer": 6,
      "text": "$text_label",
      "font_type": "$default_font"
   },
