{
    "namespace": "minerain_template",
    "light_content_toggle": {
        "type": "panel",
        "size": [
            "100%",
            "100%"
        ],
        "$toggle_text|default": "你妈死了",
        "$toggle_size|default": [
            "100%-2px",
            "100%-2px"
        ],
        "$view_binding_name|default": "none",
        "$all_layer|default": 5,
        "$is_bg_black|default": false,
        "$text_font_type|default": "$default_font",
        "$is_last|default": false,
        "$radio_toggle_group": true,
        "$toggle_name": "menu_toggle",
        "$toggle_group_default_selected": 0,
        "$toggle_focus_enabled": true,
        "$has_double_click_function|default": false,
        "$binding_type": "global",
        "$binding_condition": "once",
        "$toggle_type|default": "minerain_template.text_template",
        "$default_toggle_texture|default": "textures/MineRain/white",
        "$default_hover_toggle_texture|default": "textures/MineRain/white_hover",
        "$pressed_toggle_texture|default": "textures/MineRain/black",
        "$pressed_hover_toggle_texture|default": "textures/MineRain/black_hover",
        "$default_locked_toggle_texture|default": "textures/MineRain/red",
        "$default_locked_hover_toggle_texture|default": "textures/MineRain/red",
        "$pressed_locked_toggle_texture|default": "textures/MineRain/red",
        "$pressed_locked_hover_toggle_texture|default": "textures/MineRain/red",
        "$default_toggle_alpha|default": 0.5,
        "$default_hover_toggle_alpha|default": 0.5,
        "$pressed_toggle_alpha|default": 0.5,
        "$pressed_hover_toggle_alpha|default": 0.5,
        "$default_locked_toggle_alpha|default": 0.65,
        "$default_locked_hover_toggle_alpha|default": 0.65,
        "$pressed_locked_toggle_alpha|default": 1,
        "$pressed_locked_hover_toggle_alpha|default": 1,
        "$default_text_alpha|default": 1,
        "$default_hover_text_alpha|default": 1,
        "$pressed_text_alpha|default": 1,
        "$pressed_hover_text_alpha|default": 1,
        "$default_locked_text_alpha|default": 1,
        "$default_locked_hover_text_alpha|default": 1,
        "$pressed_locked_text_alpha|default": 1,
        "$pressed_locked_hover_text_alpha|default": 1,
        "$default_icon_alpha|default": 1,
        "$default_hover_icon_alpha|default": 1,
        "$pressed_icon_alpha|default": 1,
        "$pressed_hover_icon_alpha|default": 1,
        "$default_locked_icon_alpha|default": 1,
        "$default_locked_hover_icon_alpha|default": 1,
        "$pressed_locked_icon_alpha|default": 1,
        "$pressed_locked_hover_icon_alpha|default": 1,
        "$default_text_color|default": [
            0,
            0,
            0
        ],
        "$default_hover_text_color|default": [
            0,
            0,
            0
        ],
        "$pressed_text_color|default": [
            1,
            1,
            1
        ],
        "$pressed_hover_text_color|default": [
            1,
            1,
            1
        ],
        "$default_locked_text_color|default": [
            1,
            1,
            1
        ],
        "$default_locked_hover_text_color|default": [
            1,
            1,
            1
        ],
        "$pressed_locked_text_color|default": [
            1,
            1,
            1
        ],
        "$pressed_locked_hover_text_color|default": [
            1,
            1,
            1
        ],
        "$default_icon_color|default": [
            0,
            0,
            0
        ],
        "$default_hover_icon_color|default": [
            0,
            0,
            0
        ],
        "$pressed_icon_color|default": [
            1,
            1,
            1
        ],
        "$pressed_hover_icon_color|default": [
            1,
            1,
            1
        ],
        "$default_locked_icon_color|default": [
            1,
            1,
            1
        ],
        "$default_locked_hover_icon_color|default": [
            1,
            1,
            1
        ],
        "$pressed_locked_icon_color|default": [
            1,
            1,
            1
        ],
        "$pressed_locked_hover_icon_color|default": [
            1,
            1,
            1
        ],
        "variables": [
            {
                "requires": "$is_bg_black",
                "$default_toggle_texture|default": "textures/MineRain/black",
                "$default_hover_toggle_texture|default": "textures/MineRain/black_hover",
                "$pressed_toggle_texture|default": "textures/MineRain/white",
                "$pressed_hover_toggle_texture|default": "textures/MineRain/white_hover",
                "$default_text_color|default": [
                    1,
                    1,
                    1
                ],
                "$default_hover_text_color|default": [
                    1,
                    1,
                    1
                ],
                "$pressed_text_color|default": [
                    0,
                    0,
                    0
                ],
                "$pressed_hover_text_color|default": [
                    0,
                    0,
                    0
                ],
                "$default_locked_text_color|default": [
                    1,
                    1,
                    1
                ],
                "$default_locked_hover_text_color|default": [
                    1,
                    1,
                    1
                ],
                "$pressed_locked_text_color|default": [
                    1,
                    1,
                    1
                ],
                "$pressed_locked_hover_text_color|default": [
                    1,
                    1,
                    1
                ],
                "$default_icon_color|default": [
                    1,
                    1,
                    1
                ],
                "$default_hover_icon_color|default": [
                    1,
                    1,
                    1
                ],
                "$pressed_icon_color|default": [
                    0,
                    0,
                    0
                ],
                "$pressed_hover_icon_color|default": [
                    0,
                    0,
                    0
                ],
                "$default_locked_icon_color|default": [
                    1,
                    1,
                    1
                ],
                "$default_locked_hover_icon_color|default": [
                    1,
                    1,
                    1
                ],
                "$pressed_locked_icon_color|default": [
                    1,
                    1,
                    1
                ],
                "$pressed_locked_hover_icon_color|default": [
                    1,
                    1,
                    1
                ]
            }
        ],
        "$default_control": "minerain_template.minerain_toggle_template_default",
        "$default_hover_control": "minerain_template.minerain_toggle_template_default_hover",
        "$pressed_control": "minerain_template.minerain_toggle_template_pressed",
        "$pressed_hover_control": "minerain_template.minerain_toggle_template_pressed_hover",
        "$default_locked_control": "minerain_template.minerain_toggle_template_default_locked",
        "$default_locked_hover_control": "minerain_template.minerain_toggle_template_default_locked_hover",
        "$pressed_locked_control": "minerain_template.minerain_toggle_template_pressed_locked",
        "$pressed_locked_hover_control": "minerain_template.minerain_toggle_template_pressed_hover",
        "controls": [
            {
                "$view_binding_name@common.toggle": {
                    "size": "$toggle_size",
                    "$checked_locked_control": "$pressed_locked_control",
                    "$unchecked_locked_hover_control": "$default_locked_hover_control",
                    "$checked_locked_hover_control": "$pressed_locked_hover_control",
                    "$unchecked_locked_control": "$default_locked_control",
                    "$unchecked_control": "$default_control",
                    "$checked_control": "$pressed_control",
                    "$unchecked_hover_control": "$default_hover_control",
                    "$checked_hover_control": "$pressed_hover_control",
                    "button_mappings": [
                        {
                            "from_button_id": "button.menu_select",
                            "to_button_id": "button.menu_select",
                            "mapping_type": "pressed"
                        },
                        {
                            "ignored": "(not $is_last)",
                            "from_button_id": "button.menu_cancel",
                            "to_button_id": "button.menu_select",
                            "mapping_type": "global"
                        },
                        {
                            "from_button_id": "button.menu_ok",
                            "to_button_id": "button.menu_ok",
                            "mapping_type": "focused"
                        }
                    ]
                }
            }
        ]
    },
    "text_template": {
        "type": "label",
        "color": "$text_color",
        "text": "$toggle_text",
        "layer": "($all_layer + 1)",
        "alpha": "$text_alpha",
        "font_type": "$text_font_type"
    },
    "minerain_toggle_template": {
        "type": "image",
        "size": [
            "100%",
            "100%"
        ],
        "texture": "$toggle_texture",
        "alpha": "$toggle_alpha",
        "layer": "$all_layer",
        "controls": [
            {
                "toggle_type@$toggle_type": {}
            }
        ]
    },
    "minerain_toggle_template_default@minerain_template.minerain_toggle_template": {
        "$toggle_texture": "$default_toggle_texture",
        "$toggle_alpha": "$default_toggle_alpha",
        "$text_color": "$default_text_color",
        "$text_alpha": "$default_text_alpha",
        "$icon_color": "$default_icon_color",
        "$icon_alpha": "$default_icon_alpha"
    },
    "minerain_toggle_template_default_hover@minerain_template.minerain_toggle_template": {
        "$toggle_texture": "$default_hover_toggle_texture",
        "$toggle_alpha": "$default_hover_toggle_alpha",
        "$text_color": "$default_hover_text_color",
        "$text_alpha": "$default_hover_text_alpha",
        "$icon_color": "$default_hover_icon_color",
        "$icon_alpha": "$default_hover_icon_alpha"
    },
    "minerain_toggle_template_pressed@minerain_template.minerain_toggle_template": {
        "$toggle_texture": "$pressed_toggle_texture",
        "$toggle_alpha": "$pressed_toggle_alpha",
        "$text_color": "$pressed_text_color",
        "$text_alpha": "$pressed_text_alpha",
        "$icon_color": "$pressed_icon_color",
        "$icon_alpha": "$pressed_icon_alpha"
    },
    "minerain_toggle_template_pressed_hover@minerain_template.minerain_toggle_template": {
        "$toggle_texture": "$pressed_hover_toggle_texture",
        "$toggle_alpha": "$pressed_hover_toggle_alpha",
        "$text_color": "$pressed_hover_text_color",
        "$text_alpha": "$pressed_hover_text_alpha",
        "$icon_color": "$pressed_hover_icon_color",
        "$icon_alpha": "$pressed_hover_icon_alpha"
    },
    "minerain_toggle_template_default_locked@minerain_template.minerain_toggle_template": {
        "$toggle_texture": "$default_locked_toggle_texture",
        "$toggle_alpha": "$default_locked_toggle_alpha",
        "$text_color": "$default_locked_text_color",
        "$text_alpha": "$default_locked_text_alpha",
        "$icon_color": "$default_locked_icon_color",
        "$icon_alpha": "$default_locked_icon_alpha"
    },
    "minerain_toggle_template_default_locked_hover@minerain_template.minerain_toggle_template": {
        "$toggle_texture": "$default_locked_hover_toggle_texture",
        "$toggle_alpha": "$default_locked_hover_toggle_alpha",
        "$text_color": "$default_locked_hover_text_color",
        "$text_alpha": "$default_locked_hover_text_alpha",
        "$icon_color": "$default_locked_hover_icon_color",
        "$icon_alpha": "$default_locked_hover_icon_alpha"
    },
    "minerain_toggle_template_pressed_locked@minerain_template.minerain_toggle_template": {
        "$toggle_texture": "$pressed_locked_toggle_texture",
        "$toggle_alpha": "$pressed_locked_toggle_alpha",
        "$text_color": "$pressed_locked_text_color",
        "$text_alpha": "$pressed_locked_text_alpha",
        "$icon_color": "$pressed_locked_icon_color",
        "$icon_alpha": "$pressed_locked_icon_alpha"
    },
    "minerain_toggle_template_pressed_locked_hover@minerain_template.minerain_toggle_template": {
        "$toggle_texture": "$pressed_locked_hover_toggle_texture",
        "$toggle_alpha": "$pressed_locked_hover_toggle_alpha",
        "$text_color": "$pressed_locked_hover_text_color",
        "$text_alpha": "$pressed_locked_hover_text_alpha",
        "$icon_color": "$pressed_locked_hover_icon_color",
        "$icon_alpha": "$pressed_locked_hover_icon_alpha"
    }
}